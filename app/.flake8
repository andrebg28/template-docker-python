[flake8]
max-line-length = 100
ignore = E203, W503
select = C,E,F,W,B,B950
per-file-ignores =
    __init__.py: F401
exclude = 
    .git,
    __pycache__,
    env,
    venv,
    .venv,
    .mypy_cache,
    .pytest_cache,
    .tox,
    .nox,
    .vscode,
    .idea,
    build,
    dist,
    *.egg-info
per-file-ignores =
    __init__.py:F401
max-complexity = 10